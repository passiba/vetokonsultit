    // Code for dispatching events from components to event handlers.

    private class FormListener implements java.awt.event.ActionListener {
        FormListener() {}
        public void actionPerformed(java.awt.event.ActionEvent evt) {
            if (evt.getSource() == tunnusField) {
                JasentietoDetailForm.this.tunnusFieldActionPerformed(evt);
            }
            else if (evt.getSource() == saveButton) {
                JasentietoDetailForm.this.saveButtonActionPerformed(evt);
            }
            else if (evt.getSource() == refreshButton) {
                JasentietoDetailForm.this.refreshButtonActionPerformed(evt);
            }
            else if (evt.getSource() == newJasenTyyppiButton) {
                JasentietoDetailForm.this.newJasenTyyppiButtonActionPerformed(evt);
            }
            else if (evt.getSource() == deleteButton) {
                JasentietoDetailForm.this.deleteButtonActionPerformed(evt);
            }
            else if (evt.getSource() == newButtonJasenTyyppi) {
                JasentietoDetailForm.this.newButtonJasenTyyppiActionPerformed(evt);
            }
            else if (evt.getSource() == deleteButtonJasanTyyppi) {
                JasentietoDetailForm.this.deleteButtonJasanTyyppiActionPerformed(evt);
            }
            else if (evt.getSource() == refreshButtonJasenTyyppi) {
                JasentietoDetailForm.this.refreshButtonJasenTyyppiActionPerformed(evt);
            }
            else if (evt.getSource() == saveButtonJasenTyyppi) {
                JasentietoDetailForm.this.saveButtonJasenTyyppiActionPerformed(evt);
            }
        }
    }// </editor-fold>//GEN-END:initComponents

    

    @SuppressWarnings("unchecked")
    private void refreshButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_refreshButtonActionPerformed
        entityManager.getTransaction().rollback();
        entityManager.getTransaction().begin();
        java.util.Collection data = queryJasenet.getResultList();
        for (Object entity : data) {
            entityManager.refresh(entity);
        }
        listJasenet.clear();
        listJasenet.addAll(data);
    }//GEN-LAST:event_refreshButtonActionPerformed
    
    private void deleteButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_deleteButtonActionPerformed
        int[] selected = masterJasenetTable.getSelectedRows();
        List<vetokonsultit.olioharjoitukset.entities.Jasen> toRemove = new ArrayList<vetokonsultit.olioharjoitukset.entities.Jasen>(selected.length);
        for (int idx = 0; idx < selected.length; idx++) {
            vetokonsultit.olioharjoitukset.entities.Jasen j = listJasenet.get(masterJasenetTable.convertRowIndexToModel(selected[idx]));
            toRemove.add(j);
            entityManager.remove(j);
        }
        listJasenet.removeAll(toRemove);
    }//GEN-LAST:event_deleteButtonActionPerformed
    
    private void newJasenTyyppiButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_newJasenTyyppiButtonActionPerformed
        vetokonsultit.olioharjoitukset.entities.Jasen j = new vetokonsultit.olioharjoitukset.entities.Jasen();
        entityManager.persist(j);
        listJasenet.add(j);
        int row = listJasenet.size() - 1;
        masterJasenetTable.setRowSelectionInterval(row, row);
        masterJasenetTable.scrollRectToVisible(masterJasenetTable.getCellRect(row, 0, true));
    }//GEN-LAST:event_newJasenTyyppiButtonActionPerformed
    
    private void saveButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_saveButtonActionPerformed
        try {
            entityManager.getTransaction().commit();
            entityManager.getTransaction().begin();
        } catch (RollbackException rex) {
            rex.printStackTrace();
            entityManager.getTransaction().begin();
            List<vetokonsultit.olioharjoitukset.entities.Jasen> merged = new ArrayList<vetokonsultit.olioharjoitukset.entities.Jasen>(listJasenet.size());
            for (vetokonsultit.olioharjoitukset.entities.Jasen j : listJasenet) {
                merged.add(entityManager.merge(j));
            }
            listJasenet.clear();
            listJasenet.addAll(merged);
        }
    }//GEN-LAST:event_saveButtonActionPerformed

private void newButtonJasenTyyppiActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_newButtonJasenTyyppiActionPerformed
        vetokonsultit.olioharjoitukset.entities.Jasentyyppi j = new vetokonsultit.olioharjoitukset.entities.Jasentyyppi();
        
        j.setTunnus(this.jTextFieldJasenTyyppi.getText());
        j.setSelite(this.jTextFieldJasentyyppiSelite.getText());
        entityManager.persist(j);
        listJasenTyypit.add(j);
        int row = listJasenTyypit.size() - 1;
        
        masterJasenTyyppiTable.setRowSelectionInterval(row, row);
       
        masterJasenTyyppiTable.scrollRectToVisible(masterJasenetTable.getCellRect(row, 0, true));
    
}//GEN-LAST:event_newButtonJasenTyyppiActionPerformed

private void deleteButtonJasanTyyppiActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_deleteButtonJasanTyyppiActionPerformed
        int[] selected = masterJasenTyyppiTable.getSelectedRows();
        List<vetokonsultit.olioharjoitukset.entities.Jasentyyppi> toRemove = new ArrayList<vetokonsultit.olioharjoitukset.entities.Jasentyyppi>(selected.length);
        for (int idx = 0; idx < selected.length; idx++) {
            vetokonsultit.olioharjoitukset.entities.Jasentyyppi j =(vetokonsultit.olioharjoitukset.entities.Jasentyyppi) listJasenTyypit.get(masterJasenTyyppiTable.convertRowIndexToModel(selected[idx]));
             
            toRemove.add(j);
            toRemove.add(j);
            entityManager.remove(j);
        }
       listJasenTyypit.removeAll(toRemove);
}//GEN-LAST:event_deleteButtonJasanTyyppiActionPerformed

private void refreshButtonJasenTyyppiActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_refreshButtonJasenTyyppiActionPerformed
        entityManager.getTransaction().rollback();
        entityManager.getTransaction().begin();
        java.util.Collection data = queryJasenTyypit.getResultList();
        for (Object entity : data) {
            entityManager.refresh(entity);
        }
       listJasenTyypit.clear();
        listJasenTyypit.addAll(data);
}//GEN-LAST:event_refreshButtonJasenTyyppiActionPerformed

private void saveButtonJasenTyyppiActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_saveButtonJasenTyyppiActionPerformed
        try {
            entityManager.getTransaction().commit();
            entityManager.getTransaction().begin();
        } catch (RollbackException rex) {
            rex.printStackTrace();
            entityManager.getTransaction().begin();
            List<vetokonsultit.olioharjoitukset.entities.Jasentyyppi> merged = new ArrayList<vetokonsultit.olioharjoitukset.entities.Jasentyyppi>(listJasenTyypit.size());
            /*for (vetokonsultit.olioharjoitukset.entities.Jasentyyppi j : listJasenTyypit) {
                merged.add(entityManager.merge(j));
            }*/
           listJasenTyypit.clear();
            listJasenTyypit.addAll(merged);
        }
}//GEN-LAST:event_saveButtonJasenTyyppiActionPerformed

